{
  "name": "meetupjs-ar.github.io",
  "version": "2.41.0",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build-prod": "NODE_ENV=production REACT_APP_VERSION=${npm_package_version} react-app-rewired build",
    "dev": "REACT_APP_VERSION=${npm_package_version} env-cmd .env react-app-rewired start",
    "precommit": "lint-staged",
    "start": "serve build --single"
  },
  "author": "durancristhian@gmail.com",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --arrow-parens always --print-width 100 --single-quote --write",
      "git add"
    ]
  },
  "now": {
    "env": {
      "NODE_ENV": "production"
    },
    "files": [
      "build"
    ],
    "public": true,
    "scale": {
      "sfo1": {
        "min": 1,
        "max": 1
      }
    },
    "static": {
      "headers": [
        {
          "source": "**/static/**/*.*",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=86400"
            }
          ]
        }
      ],
      "trailingSlash": true
    },
    "type": "npm"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.15.0-1",
    "@mdx-js/mdx": "^0.15.0-1",
    "boxicons": "^1.7.1",
    "date-fns": "^1.29.0",
    "env-cmd": "^8.0.2",
    "eslint-plugin-prettier": "^2.6.2",
    "lint-staged": "^7.2.0",
    "mousetrap": "^1.6.2",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.1",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-tilt": "^0.1.4",
    "smoothscroll-polyfill": "^0.4.3",
    "source-map-explorer": "^1.5.0",
    "stylelint": "^9.4.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-standard": "^18.2.0",
    "tachyons": "^4.11.1"
  },
  "description": ":earth_americas: Sitio oficial de Meetup.js Argentina",
  "dependencies": {
    "serve": "^9.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetupjs-ar/meetupjs-ar.github.io.git"
  },
  "keywords": [
    "meetup",
    "javascript",
    "buenos aires"
  ],
  "bugs": {
    "url": "https://github.com/meetupjs-ar/meetupjs-ar.github.io/issues"
  },
  "homepage": "http://meetupjs.com.ar/"
}
